{
  "extends": "astro/tsconfigs/strictest",
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "baseUrl": ".",
    "paths": {
      "@/*": ["src/*"],
      "@/styles/*": ["src/styles/*"],
      "@/layouts/*": ["src/layouts/*"],
      "@/components/*": ["src/components/*"],
      "@/components": ["src/components"],
      "@/libs": ["src/libs"],
      "@/helpers": ["src/helpers"],
      "@/contexts": ["src/contexts"]
    }
  }
}
